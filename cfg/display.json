{
  "use": true,
  "js": [],
  "debounce": 50,
  "toggle": "navbar-expand",
  "dom": [
    {
      "id": "socialsites",
      "J_selector": ".peconic-social-sites",
      "J_parent": ".pec-nav-social-col-head",
      "triggers": [
        {
          "checker": {
            "J_selector": "body",
            "has": true,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-menu-extras",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        },
        {
          "checker": {
            "J_selector": "body",
            "has": false,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-social-col-head",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        }
      ]
    },
    {
      "id": "search",
      "J_selector": ".pec-search-cont",
      "J_parent": ".pec-head-bar-right",
      "triggers": [
        {
          "checker": {
            "J_selector": "body",
            "has": true,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-header-navbar",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": false
        },
        {
          "checker": {
            "J_selector": "body",
            "has": false,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-head-bar-right",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        }
      ]
    },
    {
      "id": "navbar",
      "J_selector": ".navbar",
      "J_parent": ".pec-nav-nav-col",
      "triggers": [
        {
          "checker": {
            "J_selector": "body",
            "has": true,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-nav-col",
          "class": {
            "add": "col",
            "remove": ""
          },
          "show": true,
          "append": true
        },
        {
          "checker": {
            "J_selector": "body",
            "has": false,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-nav-col",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        }
      ]
    },
    {
      "id": "navbarcontainer",
      "J_selector": ".pec-nav-nav-col",
      "J_parent": ".pec-nav-nav",
      "triggers": [
        {
          "checker": {
            "J_selector": "body",
            "has": true,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-nav",
          "class": {
            "add": "row",
            "remove": "col"
          },
          "show": true,
          "append": true
        },
        {
          "checker": {
            "J_selector": "body",
            "has": false,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-nav-nav",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        }
      ]
    },
    {
      "id": "toplogo",
      "J_selector": ".navbar-brand",
      "J_parent": ".pec-logo-header",
      "triggers": [
        {
          "checker": {
            "J_selector": "body",
            "has": true,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".navbar",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": false
        },
        {
          "checker": {
            "J_selector": "body",
            "has": false,
            "class": "menu-toggler-visible"
          },
          "J_parent": ".pec-logo-header",
          "class": {
            "add": "",
            "remove": ""
          },
          "show": true,
          "append": true
        }
      ]
    }
  ],
  "env": {
    "class": {
      "body": "bs4-",
      "hide": "d-",
      "hidedown": "-flex",
      "hideup": "-flex"
    },
    "breakpoints": {
      "xs": [
        "",
        "sm",
        "-sm"
      ],
      "sm": [
        "xs",
        "md",
        "-md"
      ],
      "md": [
        "sm",
        "lg",
        "-lg"
      ],
      "lg": [
        "md",
        "xl",
        "-xl"
      ],
      "xl": [
        "lg",
        "",
        ""
      ]
    }
  },
  "toowide": {
      "use": true,
      "J_measure": ".navbar-nav",
      "J_against": ".navbar-expand",
      "J_addto": ".navbar-expand"
    },
  "menuscreen": {
    "use": true,
    "screen": "pec-content-overlay",
    "J_toggle": ".navbar-toggler",
    "attr": "aria-expanded",
    "value": "true"
  },
  "forceclick": {
    "use": true,
    "J_selector": ".nav-item",
    "J_find": "a",
    "attr": "href"
  },
  "resizehide": {
    "use": true,
    "items": [
      {
        "id": "navbar",
        "J_selector": ".pec-nav-nav-col",
        "J_parent": ".pec-nav-nav"
      }
    ]
  },
  "togglerstateclass": {
    "use": true,
    "J_toggle": ".navbar-toggler",
    "items": [
      {
        "id": "body",
        "J_selector": "body",
        "state": "visible",
        "class": "menu-toggler-visible"
      }
    ]
  },
  "modify": [
    {
      "J_selector": ".pec-nav-nav-col",
      "new": [
        {
          "id": "extras",
          "J_selector": ".navbar-nav",
          "append": true,
          "tag": "div",
          "attr": {
            "class": "pec-nav-menu-extras"
          }
        }
      ]
    }
  ]
}